<%= form_for @booking do |f| %>
    <%= f.hidden_field :hotel_id, value: @hotel_id %>
    <%= f.hidden_field :user_id, value: @user_id %>
    <%= f.select :rooms, options_for_select([["Select One", ""], 1, 2, 3, 4, 5])%>
    <%= f.date_field :start_date%>
    <%= f.date_field :end_date%>
    <%= f.submit %>
 <% end %>